package ch.plannr.event
{
	import ch.plannr.model.Team;
	import ch.plannr.model.User;
	
	import flash.events.Event;
	
	public class TeamSelectedEvent extends Event
	{
		public var team:Team;
		
		public function TeamSelectedEvent(team:Team)
		{
			super(Events.OWNED_TEAMS_SELECTED,true);
			this.team=team;
		}
		
	}
}