package ch.plannr.client.events
{
	import ch.plannr.client.model.User;
	
	import flash.events.Event;
	
	public class UserRegisterEvent extends Event
	{
		private var _user:User = null;
		public static const ID:String = "UserRegisterEvent";

		public function UserRegisterEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
		public function get user() : User
		{
			return _user;
		}
		
		public function set user(value:User):void
		{
			_user= value;
		} 
	}
}