# Calendar Sample
#######################################

# The default summary of a newly created event 
plannrclient.default.summary=New Event

# Main panel title
plannrclient.application.title=Calendar

# The two following properties are used by the sample launcher
plannrclient.application.name=Calendar
plannrclient.application.description = This sample shows a calendar application that displays meetings, appointments and other events for personal and professional calendars. Events can be added, deleted, moved, resized and their properties can be edited.

# buttons labels to show a day, week, work week or month
plannrclient.button.Today=Today
plannrclient.button.Day=Day
plannrclient.button.WorkWeek=Work Week
plannrclient.button.Week=Week
plannrclient.button.Month=Month

# A button label to edit a rule
plannrclient.properties.button.edit=Edit

# The Legend panel title
plannrclient.title.Legend=Legend

# The Event properties panel title
plannrclient.title.EventPanel=Edit Event

plannrclient.title.Properties=Properties

plannrclient.properties.NewEvent=New Event
plannrclient.properties.tip.NewEvent=New Event
plannrclient.properties.DeleteEvent=Delete Event
plannrclient.properties.tip.DeleteEvent=Delete Event

plannrclient.properties.Recurrence=Recurrence…
plannrclient.properties.tip.Recurrence=Recurrence…

plannrclient.properties.confirmation.recurrence.remove.title=Remove the recurrence?
plannrclient.properties.confirmation.recurrence.remove.label=Are you sure you want to remove the recurrence?
plannrclient.properties.button.remove=Remove

# The calendar label: professional or personal 
plannrclient.calendar.Professional=Professional
plannrclient.calendar.Personal=Personal

# past event label
plannrclient.legend.PastEvents=Past Events

# label of event properties fields
plannrclient.event.label.Summary=Summary:
plannrclient.event.label.StartTime=Start Time:
plannrclient.event.label.EndTime=End Time:
plannrclient.event.label.AllDay=All Day
plannrclient.event.label.Description=Description:
plannrclient.event.label.Calendar=Calendar:

plannrclient.event.hour.format=L:NN A

plannrclient.delete.warning=Are you sure you want to delete the event "{0}"?
plannrclient.delete.recurrent.warning=Are you sure you want to delete all occurrences of the event "{0}"?
plannrclient.delete.warning.plural=Are you sure you want to delete the selected events?
plannrclient.delete.title=Delete the event(s)?

plannrclient.recurrence.title=Recurrence

plannrclient.recurrence.ruleeditor.label.Interval=Interval:

# every weekday, that is every of the week except saturday and sunday
plannrclient.recurrence.ruleeditor.label.everyWeekday=Every weekday

plannrclient.recurrence.ruleeditor.first.label=first
plannrclient.recurrence.ruleeditor.second.label=second
plannrclient.recurrence.ruleeditor.third.label=third
plannrclient.recurrence.ruleeditor.fourth.label=fourth
plannrclient.recurrence.ruleeditor.fifth.label=fifth
plannrclient.recurrence.ruleeditor.last.label=last

plannrclient.recurrence.ruleeditor.month.fromStart.label=from the beginning
plannrclient.recurrence.ruleeditor.month.fromEnd.label=from the end

plannrclient.recurrence.ruleeditor.year.fromStart.label=from the beginning
plannrclient.recurrence.ruleeditor.year.fromEnd.label=from the end


# repeat "on" a specific day of week (on Monday)
plannrclient.recurrence.ruleeditor.on.label=On:

# iCalendar is a standard. The editor is manipulating event recurrence rules.
plannrclient.recurrence.ruleeditor.title.iCalendarRecRule=Recurrence Rule

# A recurrence pattern is: every day or week or month etc... 
plannrclient.recurrence.ruleeditor.title.recurrencePattern=Recurrence Pattern

# The recurrence range defines when the recurrence starts and ends 
plannrclient.recurrence.ruleeditor.title.recurrenceRange=Recurrence Range

# Every # day(s) 
# {0}: The text input to enter a number
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.daily.interval=Every {0} day(s)

# Every #  week(s) 
# {0}: The text input to enter a number
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.weekly.interval=Every {0} week(s)

# On day # from the start or end of the month 
# {0}: The day # in the month (1 to 31)
# {1}: A choice to determine if the day is from the beginning of the month or from the end
# The fromStart values are defined by:
#   plannrclient.recurrence.ruleeditor.month.fromStart.label 
#   plannrclient.recurrence.ruleeditor.month.fromEnd.label
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.monthly.choice1=On day {0} {1}

# The first/second/.../last weekday  
# {0}: the first/second/third/fifth/last
# {1}: A week day (sunday, monday ...) 
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.monthly.choice2=The {0} {1}


# Every #  months(s) 
# {0}: The text input to enter a number
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.monthly.interval=Every {0} month(s)

# Every #  year(s) 
# {0}: The text input to enter a number
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.yearly.interval=Every {0} year(s)

# On day # from the start or end of the year 
# {0}: The day # in the month (1 to 366)
# {1}: A choice to determine if the day is from the beginning of the month or from the end
# The fromStart values are defined by:
#   plannrclient.recurrence.ruleeditor.year.fromStart.label 
#   plannrclient.recurrence.ruleeditor.year.fromEnd.label
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.yearly.choice1=On day {0} {1}

# Every day # of the month  
# {0}: A month label (January, February) etc
# {1}: The day # in the month (1 to 31) 
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.yearly.choice2=Every {0} {1}


# The first/second/.../last weekday of month m 
# {0}: the first/second/third/fifth/last
# {1}: A week day (sunday, monday ...) 
# {2}: A month label (January, February) etc
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.yearly.choice3=The {0} {1} of {2}

#@ilog.nopseudo.begin
#@ilog.context.begin

# The format of the week day labels in recurrence editor panels (see DateFormatter documentation)
# ex: Monday, Tuesday... 
plannrclient.recurrence.ruleeditor.weekday.format=EEEE

#@ilog.context.end
#@ilog.nopseudo.end


#@ilog.nopseudo.begin
#@ilog.context.begin
# The format of the month labels in recurrence editor panels (see DateFormatter documentation)
# ex: January, February...
plannrclient.recurrence.ruleeditor.month.format=MMMM
#@ilog.context.end
#@ilog.nopseudo.end


#@ilog.nopseudo.begin
#@ilog.context.begin
# The format of the start time of the recurring event (see DateFormatter documentation)
plannrclient.recurrence.ruleeditor.startTime.format=MMMM D, YYYY, L:NN A
#@ilog.context.end
#@ilog.nopseudo.end


# # of occurrences of a recurrent event 
# {0}: the number of occurrences
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.count=After {0} occurrence(s)

# End date of a recurrent event 
# {0}: a date chooser
# The rest of the text will be displayed by label components. 
plannrclient.recurrence.ruleeditor.until=Until {0}


# start time of the recurrence 
plannrclient.recurrence.ruleeditor.label.start=Start:

# end time of the recurrence 
plannrclient.recurrence.ruleeditor.label.end=End:

# expert mode that shows more options
plannrclient.recurrence.ruleeditor.expertMode=Expert mode

plannrclient.recurrence.ruleeditor.end.never.label=Never

#Frequency of the recurrence: every day
plannrclient.recurrence.tab.title.daily=Daily

#Frequency of the recurrence: every week
plannrclient.recurrence.tab.title.weekly=Weekly

#Frequency of the recurrence: every month
plannrclient.recurrence.tab.title.monthly=Monthly

#Frequency of the recurrence: every year
plannrclient.recurrence.tab.title.yearly=Yearly

plannrclient.dialog.button.ok=OK
plannrclient.dialog.button.cancel=Cancel

plannrclient.dialog.notification.title=Event Notification

plannrclient.editing.confirm.dialog.title=Modify the recurrence
plannrclient.editing.confirm.dialog.modifySerie=Modify the entire series
plannrclient.editing.confirm.dialog.modifyOccurrence=Modify this particular occurrence

plannrclient.removal.confirm.dialog.title=Remove the recurrence
plannrclient.removal.confirm.dialog.modifySerie=Delete all occurrences of this event
plannrclient.removal.confirm.dialog.modifyOccurrence=Delete this particular occurrence

plannrclient.title.help=Help
plannrclient.help.content = To add an event:<ul><li>Select the time and press the <b>Enter</b> key or click the <b>New Event</b> button</li><li>Simply click the <b>New Event</b> button</li><li>Double-click in the calendar sheet</li></ul>\nTo move an event, click and drag the event to the desired time.\n\nTo resize an event, click one of its extremities and drag it to adjust its duration.\n\nTo cancel an editing gesture, keep the mouse button pressed and press the <b>Escape</b> key.\n\nThe properties of the selected event are editable in the right pane.\n\nThe recurrence of the selected event can be set or removed by clicking the <b>Recurrence…</b> button.\n\nTo filter the events of one of the calendars, check/uncheck the calendars in the legend.\n\nTo navigate in time, select a date on a date chooser. To select a range, click a date in a date chooser and Shift-click the end date.\n\nThe <b>Day</b>, <b>Work Week</b>, <b>Week</b>, and <b>Month</b> buttons allow you to change the visible time range.\n\nThe <b>Today</b> button allows you to go to the current day.\n\nThe Next and Previous buttons next to <b>Today</b> set the next or previous time range (day, week, month…).\n\nSelecting time while holding the <b>Ctrl</b> key sets the visible time range to the selected dates.\n\nThe zoom in and zoom out buttons increase/decrease the height of an hour displayed by the calendar component.   

model.summary.vacation=Vacation