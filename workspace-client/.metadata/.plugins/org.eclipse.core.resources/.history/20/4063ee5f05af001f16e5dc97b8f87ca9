package ch.plannr.client.events
{
	import ch.plannr.client.datatypes.User;
	
	import flash.events.Event;

	public class UserRegisteredEvent extends Event
	{
		private var _user:User = null;
		public static const ID:String = "UserRegisteredEvent";
		
		public function UserRegisteredEvent()
		{
			super("UserRegisteredEvent");
			//this._user=user;	
		}
		public function get user() : User
		{
			return _user;
		}
		public function set user(value:User):void
		{
			_user= value;
		} 
	}
}